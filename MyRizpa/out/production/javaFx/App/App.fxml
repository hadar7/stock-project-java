<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>

<ScrollPane fx:id="appScrollPane" fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.AppController">
   <content>
      <BorderPane fx:id="appBorderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
          <top>
              <fx:include fx:id="topSideComponent" source="../topSide/top.fxml" />
          </top>
          <right>
              <fx:include fx:id="rightSideComponent" source="../rightSide/rightSide.fxml" BorderPane.alignment="CENTER_LEFT" />
          </right>
          <bottom>
              <fx:include fx:id="bottomSideComponent" source="../bottomSide/buttomSide.fxml" />
          </bottom>
         <center>
              <fx:include fx:id="leftSideComponent" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="../leftSide/leftSide.fxml" />
         </center>
      </BorderPane>
   </content>
</ScrollPane>
